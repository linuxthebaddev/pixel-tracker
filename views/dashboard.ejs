<style>
    body {
        position: relative;
        min-height: 100vh;
        margin: 0;
        background-color: #0F0D37;
        color: white;
        font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, "Noto Sans", "Liberation Sans", sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji";
    }

    a {
        color: rgb(187, 180, 180);
    }

    .infodiv {
        text-align: center;
        background-color: #211b51;
        margin-left: 10%;
        margin-right: 10%;
        padding-top: 10px;
        padding-bottom: 10px;
        margin-top: 15px;
        border-radius: 25px;
        box-shadow: 0 2px 5px 0 rgb(0 0 0 / 16%), 0 2px 10px 0 rgb(0 0 0 / 12%) !important;
        -webkit-box-shadow: 0 2px 5px 0 rgb(0 0 0 / 16%), 0 2px 10px 0 rgb(0 0 0 / 12%) !important;
    }

    h3 {
        margin-bottom: -5px;
        font-size: 25px;
    }

    ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;

    }

    li {
        float: left;
    }

    li a {
        display: block;
        color: white;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
    }



    .content-wrap {
        padding-bottom: 5.0rem;
        /* Footer height */
    }

    .footer {
        position: absolute;
        bottom: 0;
        width: 100%;

        height: 3.0rem;
        padding-top: 1.5rem;
        padding-bottom: 1.5rem;
        background-color: #211b51;
        text-align: center;
        margin-top: 25px;
        /* Footer height */
    }
</style>
<div class="content-wrap">

    <ul>
        <li><a><b>Pixel Dashboard</b></a></li>

    </ul>
    <div style="margin-bottom: 40px;">
        <% info.forEach(userinfo=> { %>
            <div class="infodiv">
                <h3>
                    <%= userinfo.ip %>

                </h3>
                <p>
                    <b>Receiver</b>
                    <br>
                    <a><b>
                            <%= userinfo.receiver %>
                        </b></a>

                    <br>
                    <b>UserAgent</b>
                    <br>
                    <a>
                        <%= userinfo.useragent %>
                    </a>
                    <br>
                    <b>Time/Date</b>
                    <br>
                    <a>
                        <%= userinfo.time %>
                    </a>
                </p>
            </div>

            <% });%>
    </div>
</div>
<div class="footer">
    <p>Â©Linux#2455
    </p>
</div>